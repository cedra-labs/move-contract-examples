{
  "name": "@cedra-labs/dapp-sdk",
  "version": "0.0.3",
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./src/": "./src/"
  },
  "dependencies": {
    "@cedra-labs/api": "^0.0.1",
    "@cedra-labs/crypto": "^0.0.3",
    "@cedra-labs/connect-wallet-api": "^0.2.0",
    "@cedra-labs/identity-wallet-api": "^0.2.0",
    "@cedra-labs/web-transport": "^0.2.3",
    "axios": "^1.6.0",
    "uuid": "^9.0.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "@cedra-labs/ts-sdk": "^2.0.9",
    "@cedra-labs/wallet-standard": "^0.4.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.1",
    "@types/node": "^18.15.11",
    "@types/uuid": "^9.0.1",
    "axios-mock-adapter": "1.21.4",
    "jest": "^29.1.0",
    "ts-jest": "^29.1.0",
    "ts-node": "10.9.1",
    "tsup": "^8.5.0",
    "typescript": "5.4.3"
  },
  "files": [
    "dist",
    "src",
    "!src/**.test.ts",
    "!src/**/__tests__"
  ],
  "scripts": {
    "build:bundle": "tsup src/index.ts --tsconfig tsconfig.build.json --format cjs,esm --sourcemap",
    "build:declarations": "tsc --project tsconfig.build.json --emitDeclarationOnly --declaration --declarationMap",
    "build": "pnpm build:bundle && pnpm build:declarations",
    "lint": "prettier --check src",
    "lint:fix": "prettier --write src && eslint --cache ./src --fix",
    "test": "jest"
  }
}